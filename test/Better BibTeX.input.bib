@Article{Barker2003,
  Title                    = {Partial least squares for discrimination},
  Author                   = {Barker, Matthew and Rayens, William},
  File                     = {Barker2003.pdf:Barker2003.pdf:PDF},
  Groups                   = {pls,klasyfikacja},
