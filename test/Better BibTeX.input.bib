@Article{Klimczak2010,
  Title                    = {[Etiology of colorectal cancer and antioxidant barrier of the organism].},
  Author                   = {Klimczak, Alicja and Kubiak, Katarzyna and Cybulska, Magdalena and Kula, Agnieszka and Dziki, Lukasz and Malinowska, Katarzyna},
