@Article{Kaczur1997,
  Title                    = {Effect of anti-thyroid peroxidase (TPO) antibodies on TPO activity measured by chemiluminescence assay.},
  Author                   = {Kaczur, V and Vereb, G and Molnár, I and Krajczár, G and Kiss, E and Farid, N R and Balázs, C},
  Groups                   = {tarczyca, biologia},
  Journal                  = {Clinical Chemistry},
