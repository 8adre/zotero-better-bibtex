@Article{Gutkin2009,
  Title                    = {SlimPLS: A Method for Feature Selection in Gene Expression-Based Disease Classification},
  Author                   = {Gutkin, Michael and Shamir, Ron and Dror, Gideon},
  File                     = {:D\:\\ARCHIWUM\\PRACE\\Artykuly_materialy\\pls\\Gutkn2009.pdf:PDF},
