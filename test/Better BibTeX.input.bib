@Article{Ben-Dor2000,
  Title                    = {Tissue classification with gene expression profiles},
  Author                   = {Ben-Dor, A and Bruhn, L and Friedman, N and Nachman, I and Schummer, M and Yakhini, Z},
  note                  = {svm linear best},
  File                     = {Ben-Dor2000.pdf:klasyfikatory\\Ben-Dor2000.pdf:PDF},
